{
  "name": "tacn-ts-services",
  "version": "2.0.0",
  "description": "TradingAgents-CN TypeScript Service Layer",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "benchmark": "jest --testPathPattern=benchmark",
    "benchmark:report": "jest --testPathPattern=benchmark --verbose",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "start": "node build/server.js",
    "start:dev": "tsx watch src/server.ts",
    "start:prod": "node build/server.js",
    "build:server": "tsc --project tsconfig.build.json"
  },
  "keywords": [
    "trading",
    "stock-analysis",
    "ai",
    "multi-agent"
  ],
  "author": "TradingAgents-CN Team",
  "license": "PROPRIETARY",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@types/bcrypt": "^6.0.0",
    "axios": "^1.7.0",
    "bcrypt": "^6.0.0",
    "date-fns": "^3.0.0",
    "dotenv": "^16.4.5",
    "eventemitter3": "^5.0.1",
    "fastify": "^4.26.2",
    "iconv-lite": "^0.6.3",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^9.0.3",
    "mongodb": "^6.21.0",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.2.2",
    "tslib": "^2.8.1",
    "tsyringe": "^4.8.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/iconv-lite": "^0.0.1",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
